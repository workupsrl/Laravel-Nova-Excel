{
  "name": "workup/laravel-nova-excel",
  "description": "Supercharged Excel exports for Laravel Nova Resources",
  "license": "MIT",
  "keywords": [
    "laravel",
    "phpspreadsheet",
    "phpexcel",
    "nova",
    "laravel-nova",
    "actions"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.27",
    "laravie/serialize-queries": "^1.0|^2.0",
    "maatwebsite/excel": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Workup\\Nova\\LaravelNovaExcel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Workup\\Nova\\LaravelNovaExcel\\LaravelNovaExcelServiceProvider"
      ]
    }
  }
}
